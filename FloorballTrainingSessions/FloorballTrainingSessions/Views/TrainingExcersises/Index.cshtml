@model IEnumerable<FloorballTrainingSessions.TrainingExcersises>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Excersises.ExcersiseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Trainings.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TrainingSchemePartModels.PartLength)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Excersises.ExcersiseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Trainings.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TrainingSchemePartModels.PartLength)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
