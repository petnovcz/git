@model IEnumerable<MyMoney.Outcomes>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PlannedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlannedAmount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RealDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RealAmount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Closed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accounts.AccountName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OutcomeCategories.OutcomeCategoryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Periods.PeriodName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Years.YearName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PlannedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PlannedAmount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RealDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RealAmount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Closed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accounts.AccountName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OutcomeCategories.OutcomeCategoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Periods.PeriodName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Years.YearName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
